<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>PaperMID</title>
    <meta name="description" content="Material design landing page template built by TemplateFlip.com" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Style/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Style/css/mdb.min.css" rel="stylesheet" />
    <link href="~/Style/styles/main.css" rel="stylesheet" />
    <link href="~/Style/css/jquery.gmaps.css" rel="stylesheet" />
    <style>
        .cabe {
            background-color: rgba(3, 169, 244, .30);
            color: #03A9F4;
        }

        .acento {
            background: #CDDC39;
        }

        .txtlink {
            color: #03A9F4;
        }

        .prod {
            border-radius: 20px;
            background-color: #EC2C91;
            margin-left: 8px;
            font-weight: bold;
            color: white;
            text-align: center;
        }

        .qui {
            border-radius: 20px;
            background-color: #96258D;
            margin-left: 8px;
            font-weight: bold;
            text-align: center;
        }

        .cont {
            border-radius: 20px;
            background-color: #CBD93E;
            margin-left: 8px;
            font-weight: bold;
            text-align: center;
        }

        .ini {
            border-radius: 20px;
            background-color: #05B1BB;
            margin-left: 8px;
            font-weight: bold;
            text-align: center;
        }

        .reg {
            background-color: #FCB817;
            margin-left: 8px;
            font-weight: bold;
            text-align: center;
        }

        .pos {
            position: absolute;
        }
    </style>
</head>
<body>
    <header>
        <!-- Navbar-->
        <nav class="navbar navbar-expand-lg navbar-dark fixed-top scrolling-navbar" id="navbar">
            <a style="margin-left:110px;" href="@Url.Action("Inicio","Publico")">
                <img src="~/Style/img/logoweb.png" style="width:30%; height:35%;" />
            </a>
            <div class="container">
                <div class="collapse navbar-collapse" id="navbarContent">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item prod"><a class="nav-link active" href="#">Productos</a></li>
                        <li class="nav-item qui"><a class="nav-link active" href="@Url.Action("QuiénesSomos","Publico")">Quiénes Somos</a></li>
                        <li class="nav-item cont"><a class="nav-link active" href="@Url.Action("Contacto","Publico")">Contacto</a></li>
                        <li class="nav-item ini"><a class="nav-link" href="@Url.Action("IniciarSesión","Publico")">Iniciar Sesión</a></li>
                    </ul><a class="btn reg btn-rounded my-0" href="@Url.Action("Registro","Publico")">Registro</a>
                </div>
            </div>
        </nav>
    </header>
    <div>
        @RenderBody()
    </div>
    <footer class="page-footer indigo darken-2 center-on-small-only pt-0 mt-0">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="mb-5 flex-center" style="align-content:center; align-items:center;">
                        <%-- Aqui van las imagenes de las marcas --%>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            <div class="container-fluid">
                <p>&copy; <a href="/">PaperMID</a> - Derechos Reservados</p>
            </div>
        </div>
    </footer>
    <script src="~/Style/js/jquery-3.2.1.min.js"></script>
    <script src="~/Style/js/popper.min.js"></script>
    <script src="~/Style/js/jquery.gmaps.js"></script>
    <script src="~/Style/js/bootstrap.min.js"></script>
    <script src="~/Style/js/mdb.js"></script>
    <script>new WOW().init();</script>
</body>
</html>
